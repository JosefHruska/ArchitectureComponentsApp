<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BaseProgressBar">
        <attr name="progressPercentage" format="integer" />
        <attr name="progressPadding" format="dimension" />
        <attr name="cornerRadius" format="integer" />
        <attr name="progressIconDimensions" format="dimension"/>
        <attr name="progressColorRes" format="reference" />
        <attr name="progressBackgroundColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="GoogleFitProgressBar">
        <attr name="progressTextAppearance" format="reference" />
        <attr name="progressIconRes" format="reference" />
    </declare-styleable>

    <declare-styleable name="TitleListButtonCard">
        <attr name="showMoreButton" format="boolean" />
        <attr name="titleText" format="string" />
        <attr name="showTitle" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CurrencySelector">
        <attr name="currencyCodeTextAppearance" format="reference" />
        <attr name="currencyIconVisible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FinishButton">
        <attr name="textColor" format="reference" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="CheckedTextView">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="IllustratedDescriptionView">
        <attr name="illustration" format="reference" />
        <attr name="title" format="string" />
        <attr name="description" format="string" />
        <attr name="titleTextAppearance" format="reference" />
        <attr name="descriptionTextAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="BulletText">
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="TitledValue">
        <attr name="name" format="string" />
        <attr name="value" format="string"/>
    </declare-styleable>


</resources>